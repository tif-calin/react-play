{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      { "runtime": "automatic" }
    ]
  ],
  "plugins": [
    [ 
      "@babel/plugin-transform-runtime", 
      { "regenerator": true }
    ],
    [
      "@babel/plugin-transform-typescript",
      { "isTSX": true, "allExtensions": true }
    ]
  ]
}